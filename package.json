{
  "name": "country-data-package",
  "version": "0.0.1",
  "description": "A Data Package for names and codes for countries and their subdivisions",
  "keywords": [
    "data package",
    "country",
    "subdivision",
    "iso 3166",
    "iso 3166-1",
    "iso 3166-2"
  ],
  "author": "James G. Kim <jgkim@jayg.org> (http://jayg.org/)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --opts tests/mocha.opts --compilers js:babel-core/register `find tests -path tests/coverage -prune -o -name '*.js' -print`",
