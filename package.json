    "babel-istanbul": "^0.5.9",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "dirty-chai": "^1.2.2",
    "eslint": "^1.10.3",
    "eslint-config-tipplr": "^0.1.9",
    "mocha": "^2.3.4",
    "mocha-clean": "^1.0.0",
    "mocha-gherkin": "^0.1.15",
    "nock": "^3.4.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0"
